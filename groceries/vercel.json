{
  "rewrites": [
    {
      "source": "/((?!_next|api|favicon.ico|manifest.json|logo.jpg|sw.js|assets).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}